{
  "name": "fmmloader26",
  "version": "1.0.9",
  "description": "A mod manager for Football Manager 2026",
  "license": "GPL-3.0",
  "author": "Justin Levine <justin@justinlevine.me> (https://justinlevine.me)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinlevinedotme/FMMLoader-26.git"
  },
  "bugs": {
    "url": "https://github.com/justinlevinedotme/FMMLoader-26/issues"
  },
  "homepage": "https://github.com/justinlevinedotme/FMMLoader-26#readme",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:debug": "npm run build && tauri build --debug",
    "build:release": "npm run build && tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:debugui": "VITE_ENABLE_DEBUG_UI=true tauri dev",
    "test:ui": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "@tauri-apps/plugin-os": "^2.0.0",
    "@tauri-apps/plugin-process": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@tauri-apps/plugin-updater": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.553.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tauri-apps/cli": "^2.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.13",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^24.1.3",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "validate-branch-name": "^1.3.2",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "src-tauri/src/**/*.rs": [
      "bash -c 'cd src-tauri && cargo fmt --'"
    ]
  },
  "validate-branch-name": {
    "pattern": "^(feature|fix|bugfix|hotfix|docs|refactor|test|chore|repo-ai)\\/[a-z0-9-]+$|^main$",
    "errorMsg": "❌ Branch name does not follow naming conventions!\n\nAllowed patterns:\n  • feature/*   - New features (e.g., feature/add-user-auth)\n  • fix/*       - Bug fixes (e.g., fix/login-error)\n  • bugfix/*    - Bug fixes alternative (e.g., bugfix/null-pointer)\n  • hotfix/*    - Urgent fixes (e.g., hotfix/security-patch)\n  • docs/*      - Documentation (e.g., docs/api-reference)\n  • refactor/*  - Refactoring (e.g., refactor/extract-utils)\n  • test/*      - Tests (e.g., test/integration-suite)\n  • chore/*     - Maintenance (e.g., chore/update-deps)\n  • main        - Main branch\n\nBranch names must be lowercase with hyphens.\nExample: feature/graphics-pack-import"
  }
}
